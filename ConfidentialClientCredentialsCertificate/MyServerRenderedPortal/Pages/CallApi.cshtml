@page
@model MyServerRenderedPortal.Pages.CallApiModel
@{
    ViewData["Title"] = "CallApi";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Call API</h1>
<h6>gets the data from the Azure AD protected Web API</h6>

@{
    if(Model.DataFromApi != null)
    {
        foreach(var item in Model.DataFromApi)
        {
            <pre>@item.TemperatureC @item.Summary </pre>
        }
    }
}


